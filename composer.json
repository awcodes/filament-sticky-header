{
    "name": "awcodes/filament-sticky-header",
    "type": "project",
    "description": "A Filament Admin plugin to make headers sticky when scrolling.",
    "keywords": [
        "framework",
        "laravel",
        "filament"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "filament/filament": "v3.0.0-alpha81",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "FilamentStickyHeader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FilamentStickyHeader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentStickyHeader\\FilamentStickyHeaderServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}